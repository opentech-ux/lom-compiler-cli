{
  "name": "lom-compiler",
  "version": "0.1.0",
  "description": "Compiler to generate playable HTML wireframe site from layout description model (LOM)",
  "main": "./lib/index.js",
  "bin": {
    "pizza": "./lib/index.js"
  },
 "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "create": "npm run build && npm run test",
    "build": "tsc -p .",
    "local": "npm i -g && pizza",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com//Gawen-pjr/LOM-compiler.git"
  },
  "keywords": [],
  "author": "Pierre-Louis JAEGER",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^16.4.13",
    "nodemon": "^2.0.12",
    "ts-node": "^10.2.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "clear": "^0.1.0",
    "commander": "^8.1.0",
    "figlet": "^1.5.0",
    "path": "^0.12.7"
  }
}
